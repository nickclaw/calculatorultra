<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
  
  <!-- Servlets -->
	 
	<servlet>
		<servlet-name>UltraServiceImpl</servlet-name>
		<servlet-class>com.calculatorultra.gwtultra.server.UltraServiceImpl</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>UltraServiceImpl</servlet-name>
		<url-pattern>gwtultra/ultraServer</url-pattern>
	</servlet-mapping>
  
  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>

</web-app>
